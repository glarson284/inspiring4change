<!DOCTYPE html>
<html>
<head>
  <title>Inspiring4change</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>  
  <nav>
    <% if user_signed_in? %>
      <li><%= link_to "Sign Out", destroy_user_session_path, method: :delete %></li>
    <% elsif  admin_signed_in? %>
      <li><%=link_to "New User", new_user_registration_path %> </li>
      <li><%= link_to "Sign Out", destroy_admin_session_path, method: :delete %></li>
    <% else %>
      <li><%= link_to "Plebs Sign In", new_user_session_path %></li>
      <li><%= link_to "Admin Sign In", new_admin_session_path %></li>
    <% end %>
  </nav>
  
  <%= yield %>

</body>
</html>
